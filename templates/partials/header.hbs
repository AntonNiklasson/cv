<header>
	<div class="header-content">
		<h1>{{title}}</h1>
		<div class="details">
			<ul class="contacts">
				{{#each contacts}}
					<li>{{this}}</li>
				{{/each}}
			</ul>
			<ul class="socials">
				{{#each socials}}
					<li><a href="{{this}}" target="_blank"><i class="fa {{@key}}"></i></a></li>
				{{/each}}
			</ul>
		</div>
	</div>
</header>
